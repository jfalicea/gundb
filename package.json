{
  "name": "gun-db",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "db-destroy": "dropdb gundb",
    "db-create": "createdb gundb",
    "db-init": "psql -f ./bin/schema.sql gundb",
    "db-reset": "npm run db-destroy && npm run db-create && npm run db-init"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.18.2",
    "helmet": "^3.22.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg-promise": "^10.5.0"
  }
}
